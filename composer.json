{
    "name": "prologue/alerts",
    "description": "Prologue Alerts is a package that handles global site messages.",
    "keywords": ["alerts", "messages", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dries Vints",
            "email": "dries.vints@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/config": "~4",
        "illuminate/session": "~4",
        "illuminate/support": "~4",
        "prologue/support": "1.0.x"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Prologue\\Alerts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prologue\\Alerts\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
